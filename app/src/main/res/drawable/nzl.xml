<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
        android:width="1200dp"
        android:height="600dp"
        android:viewportWidth="1200"
        android:viewportHeight="600">
    <group android:name="Flag_of_the_United_Kingdom">
        <group android:name="Saint_Georges_Cross">
            <path
                    android:fillColor="#FF000000"
                    android:pathData="M 300 0 L 300 300 M 0 150 L 600 150"
                    android:strokeWidth="100"
                    android:strokeColor="#FFFFFF" />
            <path
                    android:fillColor="#FF000000"
                    android:pathData="M 300 0 L 300 300 M 0 150 L 600 150"
                    android:strokeWidth="60"
                    android:strokeColor="#CC142B" />
        </group>
        <path
                android:name="Saint_Andrews_Cross"
                android:fillColor="#FF000000"
                android:pathData="M 0 0 L 600 300 M 0 300 L 600 0"
                android:strokeWidth="60"
                android:strokeColor="#FFFFFF" />
        <path
                android:name="Saint_Patricks_Cross"
                android:fillColor="#FF000000"
                android:pathData="M 0 0 L 600 300 M 0 300 L 600 0"
                android:strokeWidth="40"
                android:strokeColor="#CC142B" />
    </group>
    <group android:name="Southern_Cross">
        <group android:name="Gamma_Crucis" />
        <group
                android:pivotX="900"
                android:pivotY="240"
                android:rotation="82">
            <group android:name="Delta_Crucis" />
            <group android:name="Beta_Crucis" />
        </group>
        <group android:name="Alpha_Crucis" />
    </group>
</vector>